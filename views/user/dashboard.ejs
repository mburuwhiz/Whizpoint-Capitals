<h1>Dashboard Overview</h1>
<h2>Your Wallets</h2>
<ul>
  <% wallets.forEach(wallet => { %>
    <li><%= wallet.currency %>: <%= wallet.balance %> (Locked: <%= wallet.lockedBalance %>)</li>
  <% }) %>
</ul>

<h2>Recent Transactions</h2>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Type</th>
      <th>Amount</th>
      <th>Currency</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% transactions.forEach(tx => { %>
      <tr>
        <td><%= tx.createdAt.toDateString() %></td>
        <td><%= tx.type %></td>
        <td><%= tx.amount %></td>
        <td><%= tx.currency %></td>
        <td><%= tx.status %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
